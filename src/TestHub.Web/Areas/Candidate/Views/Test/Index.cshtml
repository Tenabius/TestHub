@model TestViewModel
@inject IPartialViewResolver Resolver

<section>
    <div class="container">
        <h3>@Model.Title</h3>
        <form id="Test" asp-action="Submit" method="post">
        @{
            foreach (QuestionViewModel content in Model.Questions)
            {
                @(await Html.PartialAsync(Resolver.GetPartialViewName(content.GetType()), content))
            }
        }
        </form>
        <input type="submit" form="Test" value="Submit"/>
    </div>
</section>